@model SkillsCodeFirstEF.Models.Classes.Skills
@{
    ViewBag.Title = "YetenekGetir";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}


<form class="form-group" method="post">
    <div>
        <label> Yetenek</label>
        @Html.TextBoxFor(x => x.Explanation, new { @class = "form-control", @style = "width:500px" })
    </div>
    <div style="margin-top:25px">
        <label>Değer</label>
        @Html.TextBoxFor(x=>x.Value,new  { @class = "form-control", @style = "width:500px" })
    </div>
    <div style="margin-top:20px">
        <button class="btn btn-primary">Güncelle</button>
    </div>
</form>

